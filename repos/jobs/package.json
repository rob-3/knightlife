{
  "name": "jobs",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:express --external:cors",
    "start": "node dist/index.js",
    "type-check": "tsc"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.14",
    "@types/express": "4.17.18",
    "esbuild": "0.19.4",
    "tsx": "3.13.0",
    "types": "file:*",
    "typescript": "5.2.2"
  }
}
